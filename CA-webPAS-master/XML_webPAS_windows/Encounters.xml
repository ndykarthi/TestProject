<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel = "tests" thread-count="1" verbose = "5">
<parameter name="testSuiteName" value="CAWEBPAS_ENCOUNTERS"></parameter>
    <!--  Test Data Creation in SOR using Virtuoso Journey and initiating through Virtuoso API   -->
   <test name = "ClinicalAide_ENCOUNTERS">
	<parameter name="journeyName" value="WebPas_IP_EC_Encounters"></parameter>
	<parameter name="SOR" value="WEBPAS"></parameter>
 	<classes>
	  	<class name="com.dedalus.catestcases.VirtuosoAPI"/> 
	</classes>
  </test> 
  
    <test name = "Clinical Aide1">  
    <parameter name="SOR" value="WEBPAS"></parameter>
 	<classes>
	  	<class name="com.dedalus.catestcases_windows.Encounters"/> 
    </classes>
  </test> 


<!--  WEBPAS - ClinicalAide - iOS / iPhone 14 / version-16 / Portrait Mode testing  -->
    <test name = "Clinical Aide2">
  	<parameter name="SOR" value="WEBPAS"></parameter>
	<parameter name="PlatformName" value="iOS"></parameter>
	<parameter name="DeviceType" value="Mobile"></parameter>
 	<parameter name="DeviceName" value="iPhone 14"></parameter> 
	<parameter name="PlatformVersion" value="16"></parameter> 
	<parameter name="DeviceOrientation" value="PORTRAIT"></parameter>
 	<classes>
	  	<class name="com.dedalus.catestcases.Encounters"/> 
    </classes>
  </test>  


    <test name = "Clinical Aide3">
    <parameter name="SOR" value="WEBPAS"></parameter>
	<parameter name="PlatformName" value="iOS"></parameter>
	<parameter name="DeviceType" value="TAB"></parameter>
 	<parameter name="DeviceName" value="iPad 10.2 (2021)"></parameter> 
	<parameter name="PlatformVersion" value="15"></parameter> 
	<parameter name="DeviceOrientation" value="LANDSCAPE"></parameter>
 	<classes>
	  	<class name="com.dedalus.catestcases.Encounters"/> 
    </classes>
  </test> 


   <test name = "Clinical Aide4">
   <parameter name="SOR" value="WEBPAS"></parameter>
	<parameter name="PlatformName" value="Android"></parameter>
	<parameter name="DeviceType" value="TAB"></parameter>
 	<parameter name="DeviceName" value="Galaxy Tab A8"></parameter> 
	<parameter name="PlatformVersion" value="13"></parameter> 
	<parameter name="DeviceOrientation" value="PORTRAIT"></parameter>
 	<classes>
	  	<class name="com.dedalus.catestcases.Encounters"/> 
    </classes>
  </test> 


   <test name = "Clinical Aide5">
   <parameter name="SOR" value="WEBPAS"></parameter>
	<parameter name="PlatformName" value="Android"></parameter>
	<parameter name="DeviceType" value="Mobile"></parameter>
 	<parameter name="DeviceName" value="Galaxy S23 Ultra"></parameter> 
	<parameter name="PlatformVersion" value="13"></parameter> 
	<parameter name="DeviceOrientation" value="PORTRAIT"></parameter>
 	<classes>
	  	<class name="com.dedalus.catestcases.Encounters"/> 
    </classes>
  </test>


</suite> 
